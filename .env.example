# ===========================================
# Book Store Environment Configuration
# Copy this file to .env and update values
# ===========================================

# PostgreSQL Database
POSTGRES_USER=bookstore
POSTGRES_PASSWORD=bookstore_secret
POSTGRES_DB=bookstore_db
POSTGRES_PORT=5432

# pgAdmin
PGADMIN_EMAIL=admin@bookstore.local
PGADMIN_PASSWORD=admin123
PGADMIN_PORT=5050

# NestJS API
NODE_ENV=development
API_PORT=3000

# Database URL (for NestJS/Prisma/TypeORM)
DATABASE_URL=postgresql://bookstore:bookstore_secret@localhost:5432/bookstore_db
